name: LightRoom
desc: Adobe LightRoom

http:
  mitm:
    - cc-api-data.adobe.io
    - photos.adobe.io
    - adobecorp.data.adobedc.net
    - api3.branch.io
    - cdn.branch.io

  script:
    - match: ^https:\/\/photos\.adobe\.io\/v2\/accounts
      name: LightRoom
      type: response
      require-body: true
      timeout: 10

script-providers:
  LightRoom:
    url: https://raw.githubusercontent.com/aleotoiday/t/main/q.js
    interval: 86400
    format: js
